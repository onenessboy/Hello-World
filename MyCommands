docker run -d --name webcontainer-ubuntu -p 80:80 webserver-ubuntu

kubectl expose deployment nginx --type=NodePort --name=nginx-site

kubeadm init --pod-network-cidr=172.31.0.0/20 --apiserver-advertise-address=15.206.80.36
 docker tag 2d767a44328c kiranmoluguri/dockercentoshtml:Mywebserver
 
 docker push kiranmoluguri/dockercentoshtml

apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx
spec:
  replicas: 2
  selector:
    matchLabels:
      app: nginx-site
  template:
    metadata:
      labels:
        app: nginx-site
    spec:
      containers:
      - name: nginx-site
        image: kiranmoluguri/kiran_1234:nginxserver
        imagePullPolicy: Always
        ports:
        - containerPort: 80
----------------------------------
apiVersion: v1
kind: Service
metadata:
  name: nginx-site
spec:
  selector:
    app: nginx-site
  ports:
  - protocol: TCP
    port: 80
    targetPort: 80
  type: NodePort
